[Unit]
Description=uvicorn service
After=network.target

[Service]
User=deploy
Group=deploy
WorkingDirectory=/srv/deploy-prj
ExecStart=/home/ubuntu/.pyenv/versions/gunicorn/bin/gunicorn config.asgi:application -w 3 -k uvicorn.workers.UvicornWorker
ExecReload=/bin/kill -s HUP $MAINPID
KillMode=mixed
TimeoutStopSec=5
PrivateTmp=true
[Install]
WantedBy=multi-user.target